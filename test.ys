        self.a = n;
    },
    getA: method(self) {
        return self.a;
    },
    print: method(self) {
        print("obj: a = ", self.a, " l.type = ", typeof(self.l), "\n");
    },
    print_fn: func(self) {
        print("obj: a = ", self.a, " l.type = ", typeof(self.l), "\n");
    }
};

obj.print();
println(obj.getA());
obj.setA(2);
obj.print();
obj.print_fn(obj);

